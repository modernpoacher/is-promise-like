{
  "name": "is-promise-like",
  "version": "0.1.15",
  "description": "Identifies a Promise or an object with a Promise-like interface",
  "main": "./index.js",
  "author": "Jonathan Perry for Modern Poacher Limited <modernpoacher@modernpoacher.com>",
  "license": "MIT",
  "engines": {
    "node": ">=12.19.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com/modernpoacher/is-promise-like.git"
  },
  "scripts": {
    "prepublishOnly": "cross-env NODE_ENV=production npm run build",
    "prebuild": "npm run clean",
    "build": "npm run babel",
    "build:watch": "npm run babel:watch",
    "clean": "rimraf lib",
    "babel": "babel src -d lib",
    "babel:watch": "npm run babel -- -w",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "cross-env NODE_ENV=test mocha test --recursive -r @babel/register --verbose",
    "pre-commit": "npm run lint && npm test && npm run build && git add lib"
  },
  "dependencies": {
    "debug": "^4.3.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.14.0",
    "@babel/eslint-parser": "^7.13.14",
    "@babel/preset-env": "^7.14.1",
    "@babel/register": "^7.13.16",
    "babel-loader": "^8.2.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "chai": "^4.3.4",
    "core-js": "^3.12.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.25.0",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "husky": "^6.0.0",
    "mocha": "^8.3.2",
    "rimraf": "^3.0.2",
    "standard": "^16.0.3"
  }
}
